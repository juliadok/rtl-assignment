      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.14",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest-enzyme": "^7.1.2",
